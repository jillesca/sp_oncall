{
  "gNMIBuddy": {
    "command": "uvx",
    "args": [
      "--from",
      "git+https://github.com/jillesca/gNMIBuddy.git",
      "gnmibuddy-mcp"
    ],
    "transport": "stdio",
    "env": {
      "NETWORK_INVENTORY": "xrd_sandbox.json",
      "LOG_LEVEL": "ERROR",
      "PYTHONUNBUFFERED": "1",
      "LOGGING_LEVEL": "ERROR",
      "MCP_LOG_LEVEL": "ERROR",
      "PYTHON_LOG_LEVEL": "ERROR",
      "GNMIBUDDY_LOG_LEVEL": "ERROR",
      "PYTHONWARNINGS": "ignore"
    }
  }
}